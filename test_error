{Example used to test if the error message containes the correct positional info}

program HelloError;
 var a, b: integer;

function twoparams (x:integer, y:real): real;
    begin return x+y end;

begin
    a := 0;
    while twoparams(1,1.0) * a / 10 do
    begin
        a := a + 1;
        writeln("value of a: ", a);
    end;
end.