program Hello1;
    function fun1 (a: integer) : integer;
    begin 
        if a>0 then 
            return 0
        else 
            begin
                while 4<10 do 
                begin
                    if a < 5 then 
                    return 3
                    else a :=5;
                end;
            end;
    end;

begin
    function fun2 (a: integer) : integer;
        begin 
            if a>0 then 
                return 0
            else 
                begin
                    function fun3 : integer;
                    begin 
                        begin
                            return 121;
                        end; 
                    end;
                    return fun3();
                end;

