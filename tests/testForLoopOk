program LoopsOk;

var result: real;

begin
    var i : integer;
    for i:= 0 to 10 do 
    begin
        result := result+i;
    end;

    var j: integer;
    for j:= 0 to 10 do  
    begin
        result := result+j;
    end;

    for j:= 0 to 10 do  
    begin
        result := result+j;
    end;
end.